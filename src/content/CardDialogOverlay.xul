<?xml-stylesheet href="chrome://global/skin/" type="text/css"?>
<!DOCTYPE overlay SYSTEM "chrome://gContactSync/locale/overlay.dtd">

<overlay id="gContactSync-abEditCardDialogOverlay"
         xmlns="http://www.mozilla.org/keymaster/gatekeeper/there.is.only.xul">
  <!-- since the tabbox doesn't have an ID, this overlay uses JavaScript to
       add a tab -->
  <script src="StringBundle.js"/>
  <script src="CardDialogOverlay.js"/>
  
  <stringbundleset id="stringbundleset">
    <stringbundle id="otherStringBundle"
                  src="chrome://gContactSync/locale/gcontactsync.properties"/>
  </stringbundleset>
  <tabpanels id="abTabPanels" flex="1">
   <vbox index="gcontactsync" flex="1">
     <vbox id="gcontactSyncFields" class="alignBoxWithFieldset">
       <groupbox flex="1">
         <caption label="&email.box;"/>
         <hbox>
           <spacer flex="1"/>
           <label control="thirdEmail" value="&third.label;" class="CardEditLabel"/>
           <hbox class="CardEditWidth">
             <textbox id="ThirdEmail" flex="1" class="uri-element"/>
             <menulist id="ThirdEmailType">
              <menupopup>
                <menuitem value="other" label="&other.label;"/>
                <menuitem value="home" label="&home.label;"/>
                <menuitem value="work" label="&work.label;"/>
              </menupopup>
            </menulist>
           </hbox>
         </hbox>
         <hbox>
           <spacer flex="1"/>
           <label control="fourthEmail" value="&fourth.label;" class="CardEditLabel"/>
           <hbox class="CardEditWidth">
             <textbox id="FourthEmail" flex="1" class="uri-element"/>
             <menulist id="FourthEmailType">
              <menupopup>
                <menuitem value="other" label="&other.label;"/>
                <menuitem value="home" label="&home.label;"/>
                <menuitem value="work" label="&work.label;"/>
              </menupopup>
            </menulist>
           </hbox>
         </hbox>
       </groupbox>
       <groupbox flex="1">
         <caption label="&screenname.box;"/>
         <hbox>
           <spacer flex="1"/>
           <label control="googleTalk" value="&googleTalk.label;" class="CardEditLabel"/>
           <hbox class="CardEditWidth">
             <textbox id="TalkScreenName" flex="1"/>
           </hbox>
         </hbox>
         <hbox>
           <spacer flex="1"/>
           <label control="icq" value="&icq.label;" class="CardEditLabel"/>
           <hbox class="CardEditWidth">
             <textbox id="ICQScreenName" flex="1"/>
           </hbox>
         </hbox>
         <hbox>
           <spacer flex="1"/>
           <label control="yahoo" value="&yahoo.label;" class="CardEditLabel"/>
           <hbox class="CardEditWidth">
             <textbox id="YahooScreenName" flex="1"/>
           </hbox>
         </hbox>
         <hbox>
           <spacer flex="1"/>
           <label control="msn" value="&msn.label;" class="CardEditLabel"/>
           <hbox class="CardEditWidth">
             <textbox id="MSNScreenName" flex="1"/>
           </hbox>
         </hbox>
         <hbox>
           <spacer flex="1"/>
           <label control="jabber" value="&jabber.label;" class="CardEditLabel"/>
           <hbox class="CardEditWidth">
             <textbox id="JabberScreenName" flex="1"/>
           </hbox>
         </hbox>
       </groupbox>
       <groupbox flex="1">
         <caption label="&numbers.box;"/>
         <hbox>
           <spacer flex="1"/>
           <label control="homeFax" value="&homeFax.label;" class="CardEditLabel"/>
           <hbox class="CardEditWidth">
             <textbox id="HomeFaxNumber" flex="1"/>
           </hbox>
         </hbox>
         <hbox>
           <spacer flex="1"/>
           <label control="otherNumber" value="&otherNum.label;" class="CardEditLabel"/>
           <hbox class="CardEditWidth">
             <textbox id="OtherNumber" flex="1"/>
           </hbox>
         </hbox>
       </groupbox>
     </vbox>
   </vbox>
   <vbox index="gcontactsync" flex="1">
     <vbox id="gcontactSyncFields" class="alignBoxWithFieldset">
       <groupbox flex="1">
         <caption label="&address.box;"/>
         <hbox>
           <spacer flex="1"/>
           <label control="fullHomeAddress" value="&home.label;" class="CardEditLabel"/>
           <hbox class="CardEditWidth">
             <textbox id="FullHomeAddress" multiline="true" wrap="virtual" flex="1"/>
           </hbox>
         </hbox>
         <hbox>
           <spacer flex="1"/>
           <label control="fullWorkAddress" value="&work.label;" class="CardEditLabel"/>
           <hbox class="CardEditWidth">
             <textbox id="FullWorkAddress" multiline="true" wrap="virtual" flex="1"/>
           </hbox>
         </hbox>
         <hbox>
           <spacer flex="1"/>
           <label control="otherAddress" value="&other.label;" class="CardEditLabel"/>
           <hbox class="CardEditWidth">
             <textbox id="OtherAddress" multiline="true" wrap="virtual" flex="1"/>
           </hbox>
         </hbox>
       </groupbox>
     </vbox>
   </vbox>
 </tabpanels> 
</overlay>
